{
  "settings": {
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.wordWrap": "on"
    },
    "[plaintext]": {
      "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
      }
    }
  },
  "extensions": {
    "recommendations": [
      "esbenp.prettier-vscode",
      "jsoncrack.vscode-jsoncrack",
      "usernamehw.errorlens",
      "vscode.json-language-features"
    ]
  }
}
