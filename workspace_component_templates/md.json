{
  "extensions": {
    "recommendations": [
      "davidanson.vscode-markdownlint",
      "esbenp.prettier-vscode",
      "yzhang.markdown-all-in-one"
    ]
  },
  "settings": {
    "[markdown]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true,
      "editor.insertSpaces": true,
      "editor.tabSize": 2,
      "editor.wordWrap": "on"
    }
  }
}
