{
  "extensions": {
    "recommendations": [
      "davidanson.vscode-markdownlint",
      "esbenp.prettier-vscode",
      "yzhang.markdown-all-in-one"
    ]
  },
  "settings": {
    "[markdown]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.wordWrap": "on"
    },
    "[plaintext]": {
      "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
      }
    }
  }
}
