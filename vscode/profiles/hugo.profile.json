{
    "name": "hugo",
    "settings": {
        "markdownlint.config": {
            "default": true
        },
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": false,
            "strings": true,
            "other": true
        },
        "editor.formatOnSave": true
    },
    "extensions": [
        "yzhang.markdown-all-in-one",
        "davidanson.vscode-markdownlint",
        "esbenp.prettier-vscode"
    ]
}
